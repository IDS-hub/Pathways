  .visuLive_sidebar.nav-sidebar li{margin: 0px 0px 10px 0;}
  .visuLive_sidebar.nav-sidebar li a{color: #ffffff; font-weight: bold; border:2px solid transparent;}
  .visuLive_sidebar.nav-sidebar li a:hover, .visuLive_sidebar.nav-sidebar li a:focus, .visuLive_sidebar.nav-sidebar li a:active, .visuLive_sidebar.nav-sidebar li a.selected{color: #ffffff; font-weight: bold; background: #11b0e4; border-radius: 2px; -moz-border-radius: 2px; -webkit-border-radius: 2px; border:2px solid #40d0ff;}
