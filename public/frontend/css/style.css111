.globalSearch{margin-top: 15px; position: relative;}
.globalSearch input[type="text"]{border-bottom: 0px; font-size: 14px; padding: 0 0 0 30px;}
.globalSearch input[type=text]:not(.browser-default):focus:not([readonly]){
  border-bottom: 1px solid #c733ff;
  -webkit-box-shadow: 0 1px 0 0 #c733ff;
