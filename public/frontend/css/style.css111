/* -- Home screen start -- */
.dashboard_container{
	background:#ffffff;
}

.topNav{position: fixed; z-index: 9; background: #ffffff; color: #333333; border:1px solid transparent; box-shadow: 0px 0px 5px #999; border-radius: 0px; height: 60px;}
.navbar-nav.cust-nav{margin-right: 13px;}
.navbar-nav.cust-nav>li>a:hover, .navbar-nav.cust-nav>.open>a, .navbar-nav.cust-nav>.open>a:focus, .navbar-nav.cust-nav>.open>a:hover {
